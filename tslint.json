{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint-react", "tslint-config-standard", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts", "coverage/lcov-report/*.js"]
  },
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true
      }
    ]
  }
}
